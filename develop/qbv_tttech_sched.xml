<interfaces xmlns="urn:ietf:params:xml:ns:yang:ietf-interfaces">
    <interface>
      <name>sw0p3</name>
      <gate-parameters xmlns="urn:ieee:std:802.1Q:yang:ieee802-dot1q-sched">
     <gate-enabled>true</gate-enabled>
     <admin-gate-states>255</admin-gate-states>
     <admin-control-list-length>4</admin-control-list-length>
     <admin-control-list>
	    <index>0</index>
	    <operation-name>set-gate-states</operation-name>
	    <sgs-params>
	       <gate-states-value>255</gate-states-value>
	       <time-interval-value>45000</time-interval-value>
	     </sgs-params>
     </admin-control-list>
     <admin-control-list>
        <index>1</index>
        <operation-name>set-and-hold-mac</operation-name>
        <shm-params>
          <gate-states-value>255</gate-states-value>
          <time-interval-value>25000</time-interval-value>
        </shm-params>
     </admin-control-list>
     <admin-control-list>
        <index>2</index>
        <operation-name>set-gate-states</operation-name>
        <sgs-params>
          <gate-states-value>240</gate-states-value>
          <time-interval-value>40000</time-interval-value>
        </sgs-params>
     </admin-control-list>
     <admin-control-list>
        <index>3</index>
        <operation-name>set-and-release-mac</operation-name>
        <srm-params>
          <gate-states-value>15</gate-states-value>
          <time-interval-value>30000  </time-interval-value>
        </srm-params>
     </admin-control-list>
     <admin-cycle-time>
        <numerator>1</numerator>
        <denominator>1000</denominator>
     </admin-cycle-time>
     <admin-cycle-time-extension>0</admin-cycle-time-extension>
     <admin-base-time>
        <seconds>0</seconds>
        <fractional-seconds>0</fractional-seconds>
     </admin-base-time>
     <config-change>true</config-change>
     </gate-parameters>
    </interface>
  </interfaces>
